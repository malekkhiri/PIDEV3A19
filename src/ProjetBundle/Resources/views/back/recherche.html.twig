{% extends "ProjetBundle:back:index2.html.twig" %}
{% block container %}
    <table class="table">


    <tr>
        <th>Id</th>
        <th>Username</th>
        <th>Email</th>
        <th>Last Login </th>
        <th>Supprimer utilisateur</th>



    </tr>
    {% for user in u %}
        <tr>
            <td>{{ user.id }}</td>
            <td>{{ user.username }}</td>
            <td>{{ user.email }}</td>
            <td>{{ user.lastLogin|date("Y-m-d") }}</td>
            <td><a href="{{ path('_sup',{'id':user.id} )}}">supprimer</a></td>



        </tr>
    {% endfor %}
{% endblock %}